import java.util.Scanner;
public class Main
{
	public static void main(String[] args)
	{
      Scanner sc=new Scanner(System.in);
      System.out.println("Enter Decimal no. :");
      int n=sc.nextInt();
      String bin="";
      int rem;
      while(n>0)
      {
          
          rem=n%2;
          bin=bin+rem;
          n=n/2;
          
      }
      System.out.println(bin);
	}
}
