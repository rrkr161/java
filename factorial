import java.util.Scanner;
public class Main
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        System.out.println("Enter a number");
     int n=s.nextInt();
   int t=1;
   for(int i=n;i>=1;i--)
   {
       t=t*i;
       
  
   }
    System.out.println(t);   
   }
    }
