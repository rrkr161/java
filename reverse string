import java.util.Scanner;
public class Main
{
    public static void main(String[] args)
    {
      Scanner s=new Scanner(System.in);
      System.out.println("Enter a char");
      String c=s.next();
      String rev="";
      int leng=c.length();
      for(int i=leng-1;i>=0;i--)
      {  
          rev=rev+c.charAt(i);
      }
      System.out.println(rev);
      
    }
}
