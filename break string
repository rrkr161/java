import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	   Scanner s=new Scanner(System.in);
	  System.out.println("Enter a Email");
	  String email=s.next();
	  String username="";
	  for(int i=0;i<email.length();i++)
	  {
	  if(email.charAt(i)=='@')
	  {
	      break;
	  }
	  else
	  {
	      username=username+email.charAt(i);
	  }
	  }
	  System.out.println("username is: "+username);
	}
}
